{
  "name": "ExampleScript",
  "author": "YourName",
  "version": "1.0.0",
  "description": "This is an example BoxJs script configuration.",
  "rules": [
    {
      "key": "example_variable",
      "type": "string",
      "default": "default_value",
      "description": "This is an example variable."
    },
    {
      "key": "example_number",
      "type": "number",
      "default": 10,
      "description": "This is an example number variable."
    },
    {
      "key": "example_boolean",
      "type": "boolean",
      "default": false,
      "description": "This is an example boolean variable."
    }
  ],
  "rewrite": [
    {
      "regex": "^https://example\\.com/api/.*$",
      "target": "https://api.example-proxy.com/"
    }
  ],
  "MITM": {
    "hostname": [
      "example.com",
      "api.example.com"
    ]
  }
}
